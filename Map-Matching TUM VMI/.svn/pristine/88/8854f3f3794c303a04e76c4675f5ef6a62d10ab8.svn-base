package com.vmi.mapmatching_demo;

public class SQLConfiguration {
	String dbName, ipAddress, username, password;

	public SQLConfiguration(String dbname, String ipAddressConfig, String usernameConfig, String passwordConfig){
		dbName = dbname;
		ipAddress = ipAddressConfig;
		username = usernameConfig;
		password = passwordConfig;
	}

	public String getDBName(){
		return dbName;
	}
	public String getIPAddress(){
		return ipAddress;
	}
	public String getUsername(){
		return username;
	}
	public String getPassword(){
		return password;
	}
}
